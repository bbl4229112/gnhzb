<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- edu.zju.cims201.GOF.hibernate.pojo.Author root -->
	<class name="edu.zju.cims201.GOF.hibernate.pojo.CalculateResult" table="calculateResult"
        discriminator-value="null">
		<id name="id" type="int" column="id">
			<generator class="increment"/>
		</id>
		
		
		<property name="userName" column="userName" type="string" />
		<property name="componentId" column="componentId" type="int" />
		<property name="stage1Emission" column="stage1Emission" type="java.lang.Double" />
		<property name="stage2Emission" column="stage2Emission" type="java.lang.Double" />
		<property name="stage3Emission" column="stage3Emission" type="java.lang.Double" />
		<property name="stage4Emission" column="stage4Emission" type="java.lang.Double" />
		<property name="stage5Emission" column="stage5Emission" type="java.lang.Double" />
		<property name="stage6Emission" column="stage6Emission" type="java.lang.Double" />
		<property name="finalResult" column="finalResult" type="java.lang.Double" />
		
		
		
				
	</class>
</hibernate-mapping>
